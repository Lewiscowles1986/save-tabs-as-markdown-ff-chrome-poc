{
  "manifest_version": 2,
  "name": "Save Tab URLs",
  "version": "0.1.2",
  "description": "Save open tabs to markdown",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "96": "icons/icon96.png",
    "128": "icons/icon.png"
  },
  "homepage_url": "https://lewiscowles.co.uk",
  "permissions": [
    "activeTab",
    "contextMenus",
    "tabs",
    "<all_urls>"
  ],
  "browser_action": {
    "default_icon": "icons/icon.png",
    "browser_style": true,
    "default_title": "Save URLs",
    "default_popup": "popup/window.html"
  }
}
